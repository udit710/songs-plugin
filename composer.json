{
    "name": "udit710/songs-plugin",
    "description": "A WordPress plugin for submitting song suggestions.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Udit Malshe",
            "email": "uditmalshe@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "composer/installers": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "udit710\\SongsPlugin\\": "includes/"
        },
        "files": [
            "includes/manage-roles-songs-plugin.php",
            "includes/suggestion-form.php"
        ]
    },
    "extra": {
        "installer-name": "songs-plugin"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
